<% if flash[:errors] %>
  <h3><%= flash[:errors] %></h3>
<% end %>

<h4> Welcome Back! Enter your login information below: </h4><br>

<%= form_tag sessions_path do %>
  <%= label_tag :username %>
  <%= text_field_tag :username %>
  <%= label_tag :password %>
  <%= password_field_tag :password %>
  <%= submit_tag "Login" %>
<% end %>
